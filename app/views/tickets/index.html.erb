<style type="text/css">
th{
width:150px;
text-align:left;
}
td{
width:150px;
}
</style>
<h1>Listing Tickets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Seat id</th>
      <th>Adress</th>
      <th>Price Paid</th>
      <th>Email adress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.seat_id_seq %></td>
        <td><%= ticket.adress %></td>
        <td><%= ticket.price_paid %></td>
        <td><%= ticket.email_adress %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are You Sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tickets', new_ticket_path %>
